{
  "metadata": {
    "spec-version": "1.5"
  },
  "display-name": "JMS",
  "configuration-groups": [
    {
      "label": "Basic",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Connection Factory",
          "name": "connectionFactory"
        },
        {
          "widget-type": "textbox",
          "label": "JMS Username",
          "name": "jmsUsername"
        },
        {
          "widget-type": "password",
          "label": "JMS Password",
          "name": "jmsPassword"
        },
        {
          "widget-type": "text",
          "label": "Provider URL",
          "name": "providerUrl"
        },
        {
          "widget-type": "select",
          "label": "Type",
          "name": "type",
          "widget-attributes": {
            "values": [
              "Queue",
              "Topic"
            ],
            "default": "Queue"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Queue/Topic name",
          "name": "name"
        },
        {
          "widget-type": "textbox",
          "label": "JNDI Context Factory",
          "name": "jndiContextFactory"
        },
        {
          "widget-type": "textbox",
          "label": "JNDI Username",
          "name": "jndiUsername"
        },
        {
          "widget-type": "password",
          "label": "JNDI Password",
          "name": "jndiPassword"
        },
        {
          "widget-type": "select",
          "label": "Message Type",
          "name": "messageType",
          "widget-attributes": {
            "values": [
              "Message",
              "Text",
              "Bytes",
              "Map"
            ],
            "default": "Text"
          }
        }
      ]
    }
  ]
}
